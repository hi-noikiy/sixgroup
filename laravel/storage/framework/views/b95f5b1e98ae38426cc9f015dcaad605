            <div class="detailWraper">
    			
    			<!-- 对应StoreApplyController.cs中的[HttpPost] -->
                <form action="/merchant-web/StoreApply/Index" id="mainForm" method="post" >
                    <input id="IsMobileEmailEditable" name="IsMobileEmailEditable" type="hidden" value="true" />
                    <!-- RUID不要了。
                    <input data-val="true" data-val-number="字段 Ruid 必须是一个数字。" id="Ruid" name="Ruid" type="hidden" value="" /> -->
                    <div class="counsel">
                        <ul>
                            <li class="tel">
